#!/usr/bin/python3

import math
import sys
import time 

def is_prime(n):
    """
    Checks if a number is prime.

    Args:
        n: The number to check.

    Returns:
        True if n is prime, False otherwise.
    """

    if n < 2:
        return False
    for i in range(2, int(math.sqrt(n)) + 1):
        if n % i == 0:
            return False
    return True

def factorize(n):
    """
    Factorizes a natural number into a product of two prime numbers.

    Args:
        n: The natural number to factorize.

    Returns:
        A tuple of the form (p, q), where p and q are prime factors of n.
    """

    for i in range(2, int(math.sqrt(n)) + 1):
        if n % i == 0 and is_prime(i):
            return (i, n // i)
    return None

if __name__ == "__main__":
    with open(sys.argv[1], "r") as f:
        for line in f:
            n = int(line.strip())

            factors = factorize(n)
            if factors:
                print("{}={}*{}".format(n, *factors))
            else:
                print("No prime factors found for {}".format(n))
